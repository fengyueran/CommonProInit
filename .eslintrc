{
  "parser": "babel-eslint",
  "extends": [
    "curacloud"
  ],
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "arrow-body-style": ["error", "as-needed"],
    "prefer-template": "off",
    "no-bitwise": 0
  },
  "globals": {
    
  }
}
